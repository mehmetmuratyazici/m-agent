{
  "name": "mmy-agent",
  "displayName": "mmy-agent",
  "description": "AI development asistant for your every project",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0",
    "node": ">=18.0.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aiSidebar",
          "title": "AI",
          "icon": "resources/ai-icon.svg"
        }
      ]
    },
    "views": {
      "aiSidebar": [
        {
          "type": "webview",
          "id": "aiSidebarView",
          "name": "AI Assistant",
          "icon": "resources/ai-icon.svg"
        }
      ]
    },
    "configuration": {
      "title": "AI Assistant Configuration",
      "properties": {
        "gemini.apiKey": {
          "type": "string",
          "default": "",
          "description": "Google Gemini API anahtarı"
        },
        "deepseek.apiKey": {
          "type": "string",
          "default": "",
          "description": "DeepSeek API anahtarı"
        },
        "anthropic.apiKey": {
          "type": "string",
          "default": "",
          "description": "Anthropic Claude API anahtarı"
        }
      }
    }
  },
  "activationEvents": [
    "onView:aiSidebarView"
  ],
  "scripts": {
    "vscode:prepublish": "NODE_NO_WARNINGS=1 npm run package",
    "compile": "NODE_NO_WARNINGS=1 webpack",
    "watch": "NODE_NO_WARNINGS=1 webpack --watch",
    "package": "NODE_NO_WARNINGS=1 webpack --mode production --devtool hidden-source-map",
    "compile-tests": "NODE_NO_WARNINGS=1 tsc -p . --outDir out",
    "watch-tests": "NODE_NO_WARNINGS=1 tsc -p . -w --outDir out",
    "pretest": "NODE_NO_WARNINGS=1 npm run compile-tests && npm run compile && npm run lint",
    "lint": "NODE_NO_WARNINGS=1 eslint src",
    "test": "NODE_NO_WARNINGS=1 vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.58.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "buffer": "^6.0.3",
    "dotenv": "^17.2.1",
    "marked": "^15.0.12",
    "openai": "^5.10.2"
  }
}
